                    "to": <%= to([["backslash", ["left_shift"]]]) %>
                },
                {
                    <%# US SpaceFn+/ to backslash, \ %>
                    "conditions": [ { "type": "variable_if", "name": "spacefn_mode", "value": 1 } ],
