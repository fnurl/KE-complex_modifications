{
  "title": "Personal rules @fnurl. All rules only apply to the internal and Apple BT.",
  "rules": [
    {
      "description": "Change caps_lock to Ctrl if pressed with other keys, to Escape if pressed alone (Internal KB and Apple BT KB).",
      "manipulators": [
        {
          "conditions": [
            {
              "type": "device_if",
              "identifiers": [
                {
                  "vendor_id": 1452,
                  "product_id": 632,
                  "description": "Internal Apple Keyboard"
                },
                {
                  "vendor_id": 1452,
                  "product_id": 598,
                  "description": "External Apple BT Keyboard"
                }
              ]
            }
          ],
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    },
